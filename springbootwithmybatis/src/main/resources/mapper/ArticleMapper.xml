<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ankang.springboot.learn.springbootwithmybatis.mapper.ArticleMapper">
    <select id="findById" parameterType="int" resultType="Article">
        SELECT * FROM springbootdata.t_article WHERE id = #{id}
    </select>
</mapper>